        <servlet-class>web.servlet.Autenticador</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Autenticador</servlet-name>
        <url-pattern>/Autenticador.action</url-pattern>
    </servlet-mapping>
    
</web-app>